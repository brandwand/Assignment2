public class LinkFinderTest {

	@Test
	public void test() {
		LinkFinder find = new LinkFinder();
		InputStream in;
		try {
			in = new FileInputStream(new File("neumont.edu"));	
			find.processPage(in);
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

}
